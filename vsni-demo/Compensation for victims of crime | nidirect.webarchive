#imagegallery {
    clear: both;
    width: 100%;
}


#imagegallery:after {
    clear: both;
    content: " ";
    display: block;
    font-size: 0;
    height: 0;
    line-height: 0;
    visibility: hidden;
    width: 0;
}

#imagegallery div {
	display: inline-block;
	float: left;
	margin: 0 12px 12px 0;
	padding: 12px;
	text-align: left;
	width: 150px;
	height: 250px;
