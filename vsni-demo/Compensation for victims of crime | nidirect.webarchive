function osCreateCookie(name,value,days) {
	// Courtesy of http://www.quirksmode.org/js/cookies.html
	if (days) {
		var date = new Date();
		date.setTime(date.getTime()+(days*24*60*60*1000));
		var expires = "; expires="+date.toGMTString();
	}
	else var expires = "";
	document.cookie = name+"="+value+expires+"; path=/";
}

function osReadCookie(name) {
	// Courtesy of http://www.quirksmode.org/js/cookies.html
	var nameEQ = name + "=";
	var ca = document.cookie.split(';');
	for(var i=0;i < ca.length;i++) {
		var c = ca[i];
		while (c.charAt(0)==' ') c = c.substring(1,c.length);
		if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
	}
	return null;
}

function smRedirect(medium) {
	
	var medium = "?medium=" + medium;
	var href = encodeURI(window.location.href);
	
	if (href.indexOf("?") > -1) {
		window.location = href.replace("?",medium + "&");
	} else if (href.indexOf("#") > -1) {
		window.location = href.replace("#",medium + "#");
	} else {
		window.location = href + medium;
	}
	
	return;
}

// Default cookie preference expiry
var smCookieExpires = 30; // 30 days

// Test cookie support
var smCookiesEnabled = false;
osCreateCookie("nid_cookie_support", 'true');
if (osReadCookie("nid_cookie_support") == 'true') smCookiesEnabled = true;

// Get cookie preference
var smCookie = osReadCookie("nid-smartphone");

// Force smartphone ?
var smForce = (window.location.search.indexOf("smartphone=yes") != -1);

// Force desktop?
var dtForce = (window.location.search.indexOf("smartphone=no") != -1);
	
// Are we showing the smartphone version already?
var inSm = (window.location.pathname.indexOf("/sm/") != -1);
	

// If no cookie preference ...

if (smCookie == null || isNaN(smCookie)) {

	// Assume smartphones want smartphone version (1) and desktops want desktop version (0)
	
	(isMobile.any())? smCookie = 1 : smCookie = 0;

	/*
	If device is not a smartphone
	and is trying to access smartphone version
	(and supports cookies)
	and has confirmed it wants to continue ...
	*/
	
	if (!isMobile.any() && inSm && smCookiesEnabled && confirm('You are about to view the mobile version of nidirect.\n\n Select OK to continue or select CANCEL to view the desktop version instead.')) {
			
		smCookie = 1; // Smartphone version preferred
	}			
}

// Are we forcing desktop or forcing mobile?
	
if (dtForce) { // desktop is forced

	smCookie = 0; // force cookie preference to desktop
	
} else if(smForce) { // smartphone/mobile is forced

	smCookie = 1; // force cookie preference to smartphone (1)
	
}

// Set the cookie

osCreateCookie("nid-smartphone", smCookie, smCookieExpires);


// Redirect if necessary
	
if(smCookie == 1 && !inSm)  {
	
	smRedirect(4);	// Redirect to smartphone
	
} else if(smCookie == 0 && inSm) {
	
	smRedirect(1);	// Redirect to desktop
}

