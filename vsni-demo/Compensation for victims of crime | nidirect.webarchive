/* CSS Reset */
html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,font,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td{margin:0;padding:0;border:0;outline:0;font-size:100%;vertical-align:baseline;background:transparent}input{border-radius:0}body{line-height:1}ol,ul{list-style:none}blockquote,q{quotes:none}blockquote:before,blockquote:after,q:before,q:after{content:'';content:none}/*:focus{outline:0}*/ins{text-decoration:none}del{text-decoration:line-through}table{border-collapse:collapse;border-spacing:0}
/* CSS Reset End */

html {
	font: normal 62.5% "Helvetica Neue", Arial, Helvetica, sans-serif;
}

body {
	margin: 0;
	padding: 0;
	font-size: 1em;
	background: url("v2-med-grey-noise.png") repeat scroll 0 0 #EAE9E8;
	color: #303030;
	color: #2e2e2e; /* To achieve sufficient contrast (3:1) with link colours */
}

/* Layout *******************************/

/* Custom containers */
.container_960{margin-left:auto;margin-right:auto;width:960px;}
.container_920{margin-left:auto;margin-right:auto;width:920px;}
.container_900{margin-left:auto;margin-right:auto;width:900px;}

/* clearing floats */
.clear{clear:both;display:block;overflow:hidden;visibility:hidden;width:0;height:0;}
.clearfix:after{clear:both;content:' ';display:block;font-size:0;line-height:0;visibility:hidden;width:0;height:0;}
.clearfix{display:inline-block;} * html .clearfix{height:1%;}.clearfix{display:block;}

/* magic dust for laying divs out in columns */
.col {  
	float: left;
	display: inline;
	position: relative;
}

#main,#sidebar {
	padding-top: 21px;
}

#main.col {
	width: 560px;
	padding: 7px 34px 24px 46px;
}

#sidebar.col {
	width: 280px;
	padding: 7px 40px 24px 0;
}

.no-sidebar #main.col {
	padding: 7px 40px 24px 46px;
}

.no-sidebar.wide-main #main.col {
	width: 874px;
}

.no-sidebar #sidebar {
	display: none;
}

.hide {
	position: absolute;
	width: 0;
	height: 0;
	overflow: hidden;
	left: -999em;
}

.nofloat { /* element should not float and be clear of other floated elements */
	clear: both !important;
