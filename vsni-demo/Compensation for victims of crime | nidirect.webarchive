	background:url("extranav-bullet.png") no-repeat 0px 5px scroll;
	padding-left:10px;
}

#survey .close {
	float:right;
}

#survey .ieclose {
	display: none;
}

/* fix introText, call to action and elink image issue 

.introText .elink, .calltoaction .elink {
	display:none;
}
*/

.calltoaction .elink {
	display:none;
}

/* Cookie Implied Consent Warning */

#cicw {
	width: 100%;
	float: left; 
	padding: 14px 0 0; 
	color: #eee; 
	background: transparent url(dk-blue-opacity-90.png) left top repeat; 
	position: absolute; 
	z-index: 10000; 
	left: 0;
}

#cicw div.container {
	max-width: 760px; 
	min-width: 320px; 
	margin: 0 auto;
}

#cicw-heading {
	float: left; 
	width: auto; 
	max-width: 300px; 
	margin: 0 10px 9px; 
	font-size: 21px;
	font-size: 2.1rem;
	line-height: 1em; 
	text-align: left;
}

#cicw-body {
	float: left; 
	min-width: 300px; 
	max-width: 540px; 
	padding: 0 10px; 
	margin: 2px 0;
}

#cicw-body p:first-child {
	margin-top: 1px;
}

#cicw-body p {
	font-size: 14px;
	font-size: 1.4rem;
	line-height: 18px;
	line-height: 1.8rem;
	margin: 1em 0;
}

#cicw-body ul {
	list-style: disc; 
	margin: 18px 1em;
}

#cicw-body ul li {
	padding: 0 0 3px;
}

#cicw-body a {
	color: #7FD4F1;
}

#cicw-body button {
	font-size: 24px; 
	padding: 0 30px; 
	margin: 6px auto;
}

.accessni-button {
	background:url("accessni-button-bkg.jpg") repeat-x scroll 0 0 #bfbfbf;
	min-height:50px;
	position:relative;
	margin-bottom: 20px;
}

.accessni-button a {
	background:url("accessni-button-arrow.png") no-repeat scroll 200px 18px transparent;
	display:block;
	text-decoration:none;
	font-weight:bold;
	font-size:18px;
	font-family:"Helvetica Neue", Arial, Helvetica, sans-serif;
	padding: 3px 40px 0 0;
    text-align: center;
	color:#102370;
}

.accessni-button a:hover {
	text-decoration:underline;
}

#warning {
    background: url("http://uxm.nidirect.gov.uk/images/error-icon.png") no-repeat scroll 30px 20px #FFEFED;
    border: 1px solid #FDB2B5;
    color: #CC0000;
    font-size: 1.4em;
    margin-bottom: 14px;
}
#warning p {
    padding-left: 4em;
    padding-right: 1em;
}

