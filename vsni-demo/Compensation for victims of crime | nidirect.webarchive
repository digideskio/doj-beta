	// Tweaked version of http://www.abeautifulsite.net/blog/2011/11/detecting-mobile-devices-with-javascript/
	// Don't want tablets
    
	Android: function() {
        return (navigator.userAgent.search(/Android/i) > -1 && navigator.userAgent.search(/Mobile/i) > -1);
    },
    iOS: function() {
        return (navigator.userAgent.search(/iPhone|iPod/i) > -1);
    },
    any: function() {
        return (isMobile.Android() || isMobile.iOS());
    }
	
