	text-decoration: none;
}

#imagegallery div a[href*=".jpg"]:after,
#imagegallery div a[href*=".jpeg"]:after {
    clear: both;
    content: "view large";
    display: block;
	position: relative;
	z-index: 100;
	width: 150px;
    font-size: 12px;
	font-size: 1.2rem;
	line-height: 20px;
	line-height: 2rem;
    top: -20px;
	top: -2rem;
	margin-bottom: -20px;
	margin-bottom: -2rem;
    text-align: center;
    color: #fff;
	background-color: rgba(0,0,0,.65);
}

