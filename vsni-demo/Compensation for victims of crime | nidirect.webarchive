	padding: 0.4286em 6px 0.4286em 6px;
}

table.simple th {
	color: #0d0348;
	text-align: left;
	vertical-align: top;
	text-shadow: 1px 1px 1px #fff;
	border: 1px solid #ccc;
}

table.simple td {
	vertical-align: top;
	border: 1px solid #ccc;
}

table.unfixed {
